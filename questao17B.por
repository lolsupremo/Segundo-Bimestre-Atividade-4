programa
{
	
	funcao inicio()
	{
		real n1 = 1, n2 = 0, contagem = 0, nvoltas
		escreva("ate qual posicao voce quer ir na sequencia de fibonacci? ")
		leia(nvoltas)
			escreva (n2, " ")
		enquanto(contagem <= nvoltas){
			n1 = n1 + n2
			escreva(n1, " ")
			n2 = n1 + n2
			escreva (n2, " ")
			contagem++ contagem++
		}
		
	}
}
